{
  "version": "2.0.1-alpha3",
  "name": "Persimmon",
  "buildOptions": {
    "debugType": "portable",
    "compilerName": "fsc",
    "keyFile": "../../Persimmon.snk",
    "define": [
      "CORE_CLR"
    ],
    "compile": {
      "includeFiles": [
        "./NonEmptyList.fsi",
        "./NonEmptyList.fs",
        "./PrettyPrinter.fs",
        "./AsyncLazy.fs",
        "./Types.fs",
        "./TestResult.fs",
        "./ActivePatterns.fs",
        "./TestCase.fs",
        "./Assertions.fs",
        "./ComputationExpressions.fs",
        "./Syntax.fs",
        "./Internals/RuntimeUtil.fs",
        "./Internals/TestCollector.fs",
        "./Internals/TestRunner.fs",
        "./Internals/Attributes.fs"
      ]
    }
  },
  "tools": {
    "dotnet-mergenupkg": "1.0.*",
    "dotnet-compile-fsc": {
      "version": "1.0.0-preview2-*",
      "imports": [
        "dnxcore50",
        "portable-net45+win81",
        "netstandard1.3"
      ]
    }
  },
  "frameworks": {
    "netstandard1.6": {
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "FSharp.Core": "4.0.1.7-alpha"
      }
    }
  }
}