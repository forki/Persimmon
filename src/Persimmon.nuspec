<?xml version="1.0" encoding="utf-8"?>
<package xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <metadata xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <id>Persimmon</id>
    <version>@build.number@</version>
    <authors>bleis-tift, Gab-km, pocketberserker, kekyo, yukitos</authors>
    <owners>bleis-tift, Gab-km, pocketberserker, kekyo, yukitos</owners>
    <licenseUrl>https://github.com/persimmon-projects/Persimmon/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/persimmon-projects/Persimmon</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/persimmon-projects/Persimmon.Materials/master/StandardIcons/persimmon_128.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A test framework for F#</description>
    <releaseNotes>@releaseNotes@</releaseNotes>
    <tags>F# fsharp testing</tags>
    <dependencies>
      <group targetFramework=".NETFramework2.0">
        <dependency id="FSharp.Core" version="3.1.2.5" />
      </group>
      <group targetFramework=".NETFramework4.0">
        <dependency id="FSharp.Core" version="3.1.2.5" />
      </group>
      <group targetFramework=".NETFramework4.5">
        <dependency id="FSharp.Core" version="3.1.2.5" />
      </group>
      <group targetFramework="portable-net45+win+wpa81+wp80+MonoAndroid10+xamarinios10+MonoTouch10">
        <dependency id="FSharp.Core" version="3.1.2.5" />
      </group>
      <group targetFramework=".NETStandard1.6">
        <dependency id="FSharp.Core" version="4.1.2" exclude="Build,Analyzers"></dependency>
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="Persimmon/Persimmon.dll" target="lib/net20" />
    <file src="Persimmon/Persimmon.XML" target="lib/net20" />
    <file src="Persimmon.NET40/Persimmon.dll" target="lib/net40" />
    <file src="Persimmon.NET40/Persimmon.XML" target="lib/net40" />
    <file src="Persimmon.NET45/Persimmon.dll" target="lib/net45" />
    <file src="Persimmon.NET45/Persimmon.XML" target="lib/net45" />
    <file src="Persimmon.NETCore/netstandard1.6/Persimmon.dll" target="lib/netstandard16" />
    <file src="Persimmon.Portable259/Persimmon.dll" target="lib/portable-net45+win+wpa81+wp80+MonoAndroid10+xamarinios10+MonoTouch10" />
    <file src="Persimmon.Portable259/Persimmon.XML" target="lib/portable-net45+win+wpa81+wp80+MonoAndroid10+xamarinios10+MonoTouch10" />
  </files>
