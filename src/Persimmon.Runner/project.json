{
  "version": "2.0.0",
  "name": "Persimmon.Runner",
  "buildOptions": {
    "debugType": "portable",
    "compilerName": "fsc",
    "keyFile": "../../Persimmon.snk",
    "define": [
      "CORE_CLR"
    ],
    "compile": {
      "includeFiles": [
        "./Writable.fs",
        "./Formatter.fs",
        "./Printer.fs",
        "./Reporter.fs",
        "./TestCollector.fs",
        "./TestRunner.fs"
      ]
    }
  },
  "dependencies": {
    "Persimmon": {
      "target": "project"
    }
  },
  "tools": {
    "dotnet-mergenupkg": "1.0.*",
    "dotnet-compile-fsc": {
      "version": "1.0.0-preview2-*",
      "imports": [
        "dnxcore50",
        "portable-net45+win81",
        "netstandard1.3"
      ]
    }
  },
  "frameworks": {
    "netstandard1.6": {
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "FSharp.Core": "4.0.1.7-alpha"
      }
    }
  }
}